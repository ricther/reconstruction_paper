	editor = {Yoshida, Hiroyuki and Warfield, Simon and Vannier, Michael W.},
	month = jan,
	year = {2013},
	keywords = {Abdominal imaging, Artificial Intelligence (incl. Robotics), Computation by Abstract Devices, Computed tomography, computer-aided diagnosis, Computer Imaging, Vision, Pattern Recognition and Graphics, Computer Science, general, {CT} colonography oncology applications, Image Processing and Computer Vision, pattern recognition, registration},
	pages = {29--38}
