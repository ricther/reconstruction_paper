	keywords = {Algorithms, Child, Computer Graphics, Diagnosis, Computer-Assisted, Encephalocele, Humans, Magnetic Resonance Imaging, Male, Models, Anatomic, Models, Theoretical, Tomography, Emission-Computed, Tomography, X-Ray Computed},
